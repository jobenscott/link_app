<h1> Posts </h1>
<p><%= link_to 'New post', new_post_path %></p>
<% @posts.each do |post| %>
  <h2><%= post.title %></h2>
  <h3><%= post.url %></h3>
  <p><%= post.vote %></p>
  <%= link_to 'Show Post', post_path(post) %>
  <%= link_to 'Edit post', edit_post_path(post) %>
  <%= link_to 'Destroy post', post, method: :delete %>
  <%= link_to 'UpVote!', posts_vote_path(id: post.id) %>
<% end %>